@model WebDeveloper.Model.Authors

<h3>Esta seguro de eliminar registro?</h3>
<div>
    <h4>Autor</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.au_lname)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.au_fname)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.au_phone)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.au_city)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.au_state)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.au_zip)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.au_sex)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.au_salary)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.au_subject)
        </dt>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Retornar", "Index")
        </div>
    }
</div>
